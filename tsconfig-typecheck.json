{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "ol": ["node_modules/ol/src"],
      "ol/*": ["node_modules/ol/src/*"]
    },
    "lib": [
      "es2015",
      "dom",
      "webworker"
    ]
  },
  "include": [
    "src/*.js",
    "node_modules/ol/**/*.js"
  ]
}